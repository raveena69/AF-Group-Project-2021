*{
    margin: 0px;
    padding: 0px;
}

.out-background{
    width: 100%;
    height: 1000px;
}

/*Top section start*/

.top-bar{
    min-width:100%;
    min-height: 722px;
    background-image: url("6f22029ad7992f8c6e1f0b3823760fa0");
    background-repeat: no-repeat;
    background-size: 100% 100%;
    background-position: center;
    float:left;
}

.top-bar1{
    width: 1200px;
    height: 722px;
    margin: auto;
    /*background: red;*/
}

.left{
    width: 520px;
    height: 610px;
    float: left;
    margin-top: 50px;
    margin-left: 50px;
    /*background: green;*/
}

.right{
    width: 520px;
    height: 610px;
    float: left;
    margin-top: 50px;
    margin-left: 50px;
    text-align: center;
    /*background: green;*/
}

.conference-name{
    width: auto;
    height: auto;
    font-family: Candara;
    color: lightpink;
    font-size: 80px;
    font-weight: bold;
    margin-top: 10px;
    text-align: center;
    /*background: cadetblue;*/
}

.still-not-registered{
    font-size: 40px;
    font-family: Bahnschrift;
    color:white;
    margin-top: 100px;
}

.sign-up{
    width: auto;
    height: 300px;
    font-family: Candara;
    color: white;
    font-size: 60px;
    font-weight: bold;
    text-decoration: none;
    margin-top: 40px;
    /*background: cadetblue;*/
}

.already-registered{
    font-size: 40px;
    font-family: Bahnschrift;
    color:white;
    margin-top: 70px;
}

.sign-in{
    width: auto;
    height: 300px;
    font-family: Candara;
    color: white;
    font-size: 60px;
    font-weight: bold;
    text-decoration: none;
    margin-top: 40px;
    /*background: cadetblue;*/
}

.sign-in:hover, .sign-up:hover{
    color: #204c69;
}

.date-back{
    width:auto;
    height: auto;
}

.dates{
    width: auto;
    height: auto;
    font-family: Candara;
    color: white;
    font-size: 35px;
    font-weight: bold;
    text-decoration: none;
    margin-top: 40px;
}

.institute{
    width: auto;
    height: auto;
    font-family: Candara;
    color: white;
    font-size: 35px;
    font-weight: bold;
    text-decoration: none;
    margin-top: 5px;
}

/*Top section end*/

/*Middle section start*/

.about-conference-back{
    width: 1200px;
    height: auto;
    margin: auto;
}

.about-conference-back1{
    width: 1200px;
    height: auto;
    float: left;
}

.about-conference{
    width: auto;
    height: auto;
    font-family: Bahnschrift;
    color: darkblue;
    font-size: 30px;
    font-weight: bold;
    margin-top: 40px;
    text-align: center;
}

.icaf-name{
    width: auto;
    height: auto;
    font-family: Bahnschrift;
    color: gray;
    font-size: 30px;
    font-weight: bold;
    margin-top: 40px;
    text-align: center;
}

/*Middle section end*/