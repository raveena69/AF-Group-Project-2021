/*Topics start*/

.topics-back{
    width:1200px;
    height: 650px;
    margin-bottom: 10px;
    margin-left: 170px;
    float: left;
}

.topics-back1{
    width:400px;
    height: 650px;
    margin: auto;
    /*background: red;*/
}

.topics{
    width: auto;
