*{
    margin: 0px;
    padding: 0px;
}

.out-background{
    width: 100%;
    height: 1000px;
}

.top-bar{
    min-width:100%;
    min-height: 722px;
    background-image: url("41f71768e3f219d0d48a6e398ed7d83a");
    background-repeat: no-repeat;
    background-size: 100% 100%;
    background-position: center;
    float:left;
}

.top-bar1{
    width: 1200px;
    height: 722px;
    background: red;
    margin: auto;
}

.left{
    width: 520px;
    height: 610px;
    float: left;
    background: green;
    margin-top: 50px;
    margin-left: 50px;
}

.right{
    width: 520px;
    height: 610px;
    float: left;
    background: green;
    margin-top: 50px;
    margin-left: 50px;
    text-align: center;
}

.conference-name{
    width: auto;
    height: auto;
    font-family: Candara;
    color: white;
    font-size: 80px;
    font-weight: bold;
    margin-top: 10px;
    background: cadetblue;
}

.still-not-registered{
    font-size: 40px;
    font-family: Bahnschrift;
    color:white;
    margin-top: 20px;
}

.sign-up{
    width: auto;
    height: 300px;
    font-family: Candara;
    color: white;
    font-size: 40px;
    font-weight: bold;
    margin-top: 10px;
    background: cadetblue;
    text-decoration: none;
}